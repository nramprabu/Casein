<!-- Scaffolding generated by Casein v.3.1.11 -->

<table cellpadding="0" cellspacing="0" id="itemList" class="itemList">
	<tr>
		<th>Name</th>
		<th>&nbsp;</th>
	</tr>
	
	<% product_categories.each_with_index do |product_category, index| %>
		<tr class="<%= 'even' if index.modulo(2) == 0 %> hover">
			<td><%= casein_table_cell_link product_category.name, casein_product_category_path(product_category) %></td>
			<td class="delete">
				<%= link_to(casein_show_row_icon("delete"), casein_product_category_path(product_category), :method => :delete, :confirm => "Are you sure you want to delete this product category?") %>
			</td>
		</tr>
	<% end %>
</table>